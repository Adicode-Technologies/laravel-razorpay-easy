{
    "name": "adicode-technologies/laravel-razorpay-easy",
    "description": "A simple and customizable Razorpay integration for Laravel applications",
    "keywords": [
        "laravel",
        "razorpay",
        "payment",
        "gateway"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adison Masih",
            "email": "therealadison@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "razorpay/razorpay": "^2.8"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "AdicodeT\\LaravelRazorpayEasy\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AdicodeT\\LaravelRazorpayEasy\\RazorpayServiceProvider"
            ],
            "aliases": {
                "Razorpay": "AdicodeT\\LaravelRazorpayEasy\\Facades\\Razorpay"
            }
        }
    },
    "prefer-stable": true
}